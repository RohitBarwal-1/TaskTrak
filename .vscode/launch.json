{
    // Will use to run and debug the application locally
    // to start just click run and debug
"version":"0.0.1",
"configurations":[
    {
        "name":"FastAPI: Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": ["api.__init__:app", "--reload"],
        "cwd": "${workspaceFolder}/server",
        "jinja": true,
        "console": "integratedTerminal",
    }
]
}